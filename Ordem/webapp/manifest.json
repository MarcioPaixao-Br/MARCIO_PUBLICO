{
    "_version": "1.12.0",
    "sap.app": {
        "id": "br.com.ordem.Ordem",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "sourceTemplate": {
            "id": "servicecatalog.connectivityComponentForManifest",
            "version": "0.0.0",
            "toolsId": "0b99a09c-a1ce-4183-aac5-32d61dabc493"
        },
        "dataSources": {
            "Northwind.svc": {
                "uri": "/kendo-ui/",
                "type": "OData",
                "settings": {
                    "localUri": "localService/metadata.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "br.com.ordem.Ordem.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        },
        "dependencies": {
            "minUI5Version": "1.65.6",
            "libs": {
                "sap.ui.layout": {},
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "br.com.ordem.Ordem.i18n.i18n"
                }
            },
            "": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultOperationMode": "Server",
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Request"
                },
                "dataSource": "Northwind.svc",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "br.com.ordem.Ordem.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "Ordem",
                    "pattern": "",
                    "titleTarget": "",
                    "greedy": false,
                    "target": [
                        "Ordem"
                    ]
                },
                {
                    "name": "Produtos",
                    "pattern": "produto/{idOrder}",
                    "titleTarget": "",
                    "greedy": false,
                    "target": [
                        "Produtos"
                    ]
                }
            ],
            "targets": {
                "TargetApp": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "App",
                    "viewName": "App"
                },
                "Ordem": {
                    "viewType": "XML",
                    "viewName": "Ordem",
                    "controlAggregation": "pages",
                    "controlId": "app",
                    "viewPath": "br.com.ordem.Ordem.view"
                },
                "Produtos": {
                    "viewType": "XML",
                    "viewName": "Produtos",
                    "controlAggregation": "pages",
                    "controlId": "app",
                    "viewPath": "br.com.ordem.Ordem.view"
                }
            }
        }
    }
}